{
  "name": "myguitare/auth",
  "description": "Force Oauth to access flarum",
  "type": "flarum-extension",
  "require": {
    "flarum/core": ">=0.1.0-beta.10 <0.1.0-beta.12"
  },
  "autoload": {
    "psr-4": {"Myguitare\\Auth\\": "src/"}
  },
  "extra": {
    "flarum-extension": {
      "title": "Force Oauth",
      "icon": {
        "name": "fas fa-smile",
        "backgroundColor": "#238c59",
        "color": "#fff"
      }
    }
  }
}